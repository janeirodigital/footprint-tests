{ "@context": "../ns/footContext.jsonld",
  "@id": "#root",
  "type":"ldp:Container", "contents":[
  {            "@id":     "#repos", "type":"ldp:Container"        , "name":"repos"                                                       , "contents":[
    {          "@id":       "#org", "type":"ldp:Container"        , "uriTemplate":"{orgName}"      , "shape": "ghSchema#PersonShape"     , "contents":[
      {        "@id":      "#repo", "type":"ldp:Container"        , "uriTemplate":"{repoName}"     , "shape": "ghSchema#RepoShape"       , "contents":[
        {      "@id":    "#issues", "type":"ldp:Container"        , "name":"issues"                , "shape": "ghSchema#C_IssueShape"    , "contents":[
          {    "@id":     "#issue", "type":"ldp:Resource"         , "uriTemplate":"{issueNo}"      , "shape": "ghSchema#IssueShape"      , "contents":[
            {  "@id":    "#cmnt_C", "type":"ldp:Container"        , "name"       :"comments"       , "shape": "ghSchema#C_LabelShape"    , "contents":[
              {"@id":    "#comnts", "type":"ldp:Resource"         , "uriTemplate":"{commentNo}"    , "shape": "ghSchema#LabelShape"      }
            ]},
            {  "@id":     "#evt_C", "type":"ldp:Container"        , "name"       :"events"         , "shape": "ghSchema#C_EventShape"    , "contents":[
              {"@id":    "#events", "type":"ldp:Resource"         , "uriTemplate":"{eventNo}"      , "shape": "ghSchema#EventShape"      }
            ]}
          ]}
        ]},
        {      "@id":     "#lbl_C", "type":"ldp:Container"        , "name":"labels"                , "shape": "ghSchema#C_LabelShape"    , "contents":[
          {    "@id":    "#labels", "type":"ldp:Resource"         , "uriTemplate":"{labelName}"    , "shape": "ghSchema#LabelShape"      }
        ]},
        {      "@id":     "#mlt_C", "type":"ldp:Container"        , "name":"milestones"            , "shape": "ghSchema#C_MilestoneShape", "contents":[
          {    "@id":"#milestones", "type":"ldp:Resource"         , "uriTemplate":"{milestoneName}", "shape": "ghSchema#MilestoneShape"  }
        ]}
      ]}
    ]}
  ]},
  {            "@id":     "#users", "type":"ldp:Container"        , "name":"users"                                                       , "contents":[
    {          "@id":      "#user", "type":"ldp:Container"        , "uriTemplate":"{userName}"     , "shape": "ghSchema#PersonShape"     , "contents":[
      {        "@id": "#followers", "type":"ldp:IndirectContainer", "name":"followers"             , "shape": "ghSchema#C_UserShape"     , "contents":[
        {      "@id":  "#follower", "type":"ldp:Resource"         , "uriTemplate":"{userName}"     , "shape": "ghSchema#PersonShape"     }
      ]},
      {        "@id": "#user-orgs", "type":"ldp:IndirectContainer", "name":"orgs"                  , "shape": "ghSchema#C_OrgShape"      , "contents":[
        {      "@id":  "#user-org", "type":"ldp:Resource"         , "uriTemplate":"{orgName}"      , "shape": "ghSchema#OrgShape"        }
      ]},
      {        "@id": "#recd-evts", "type":"ldp:IndirectContainer", "name":"received_events"       , "shape": "ghSchema#C_EventShape"    , "contents":[
        {      "@id":  "#rcvd-evt", "type":"ldp:Resource"         , "uriTemplate":"{eventName}"    , "shape": "ghSchema#EventShape"      }
      ]},
      {        "@id":"#user-repos", "type":"ldp:IndirectContainer", "name":"repos"                 , "shape": "ghSchema#C_RepoShape"     , "contents":[
        {      "@id": "#user-repo", "type":"ldp:Resource"         , "uriTemplate":"{repoName}"     , "shape": "ghSchema#RepoShape"       }
      ]},
      {        "@id":"#subscripts", "type":"ldp:IndirectContainer", "name":"subscriptions"         , "shape": "ghSchema#C_SubscrShape"   , "contents":[
        {      "@id": "#subscript", "type":"ldp:Resource"         , "uriTemplate":"{subscriptName}", "shape": "ghSchema#SubscrShape"     }
      ]}
    ]}
  ]}
]}
