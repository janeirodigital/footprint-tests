@prefix foot: <http://www.w3.org/ns/footprints#> .
@prefix ldp: <http://www.w3.org/ns/ldp#> .

<#root> a ldp:BasicContainer ; foot:contents <#image>, <#img-ref> .
   <#img-ref> a ldp:Resource       ; foot:uriTemplate "ref-{entryNo}" .
   <#image>   a ldp:BinaryResource ; foot:uriTemplate "img-{entryNo}" .
