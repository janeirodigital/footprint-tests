@prefix tree: <http://www.w3.org/ns/shapetree#> .
@prefix ldp: <http://www.w3.org/ns/ldp#> .

<#root> a ldp:BasicContainer ; tree:contents <#image>, <#img-ref> .
   <#img-ref> a ldp:Resource       ; tree:uriTemplate "ref-{entryNo}" ; tree:shape <DoesNotExist#PhotoReference> .
   <#image>   a ldp:BinaryResource ; tree:uriTemplate "img-{entryNo}" .
