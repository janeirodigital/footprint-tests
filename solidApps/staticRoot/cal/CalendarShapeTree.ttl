PREFIX ldp: <http://www.w3.org/ns/ldp#>
PREFIX tree: <http://www.w3.org/ns/shapetree#>

<#calendar> a ldp:BasicContainer ;
    tree:shape <Calendar#Calendar> ;
    tree:contents <#appointment> .

<#appointment> a ldp:Resource ;
    tree:uriTemplate "{appointmentNo}" ;
    tree:shape <Calendar#Appointment> .
