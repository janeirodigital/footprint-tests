PREFIX ldp: <http://www.w3.org/ns/ldp#>
PREFIX foot: <http://www.w3.org/ns/blueprints#>

<#calendar> a ldp:BasicContainer ;
    foot:shape <Calendar#Calendar> ;
    foot:contents <#appointment> .

<#appointment> a ldp:Resource ;
    foot:uriTemplate "{appointmentNo}" ;
    foot:shape <Calendar#Appointment> .
