{ "@context": "../ns/shapeTreeContext.jsonld",
      "@id":      "#root", "expectsType":"ldp:Container"   ,                                                             "contains":[
  {   "@id":      "#note", "expectsType":"ldp:Container"   , "matchesUriTemplate":"{noteName}", "validatedBy":"NeverNote#Note"      , "contains":[
    { "@id": "#inclusion", "expectsType":"ldp:Resource"    , "matchesUriTemplate":"inc-{name}", "validatedBy":"NeverNote#Inclusion" },
    { "@id":     "#image", "expectsType":"ldp:NonRDFSource", "matchesUriTemplate":"img-{name}" }
  ] }
] }
