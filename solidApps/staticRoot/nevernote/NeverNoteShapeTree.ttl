@prefix foot: <http://www.w3.org/ns/shapetree#> .
@prefix ldp: <http://www.w3.org/ns/ldp#> .

<#root> foot:type ldp:Container ;
  foot:contents <#note> .

<#note> foot:type ldp:Container ;
  foot:uriTemplate "{noteName}" ;
  foot:shape <NeverNote#Note> ;
  foot:contents <#inclusion>, <#image> .

<#inclusion> foot:type ldp:Resource ;
  foot:uriTemplate "inc-{name}" ;
  foot:shape <NeverNote#Inclusion> .

<#image> foot:type ldp:NonRDFSource ;
  foot:uriTemplate "img-{name}" .
