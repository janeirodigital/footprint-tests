{
  "name": "blueprint-tests",
  "version": "0.0.0",
  "keywords": [
    "solid",
    "blueprints",
    "shex",
    "shapes"
  ],
  "author": "eric@w3.org",
  "repository": "git@github.com:janeirodigital/footprint-tests",
  "license": "ISC",
  "scripts": {
    "coverage:report": "cat ./coverage/lcov.info | coveralls",
    "prepare-tests": "mkdir -p www && ls -d www/* | grep -v README.md | xargs rm -rf",
    "test": "npm run prepare-tests && PORT=12345 nyc mocha --slow 200",
    "update-www-repo": "PORT=12345 ./update-www-repo.sh",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "@shexjs/cli": "^1.0.0-alpha.8",
    "@shexjs/core": "^1.0.0-alpha.8",
    "@shexjs/parser": "^1.0.0-alpha.8",
    "chai-as-promised": "^7.1.1",
    "cookie-parser": "~1.4.4",
    "express": "~4.16.1",
    "fs-extra": "^8.1.0",
    "jsonld": "^1.8.1",
    "mocha": "^7.1.1",
    "morgan": "~1.9.1",
    "n3": "^1.3.0",
    "node-fetch": "^2.6.0",
    "relateurl": "^0.2.7",
    "request-promise": "^4.2.5",
    "serve-favicon": "^2.5.0",
    "uri-template-lite": "^19.11.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.7",
    "debug": "~2.6.9",
    "jest-silent-reporter": "^0.1.2",
    "nyc": "^15.0.0",
    "superagent": "^5.1.0"
  },
  "nyc": {
    "exclude": [
      "util/rdf-extra.js",
      "test/*"
    ]
  }
}
