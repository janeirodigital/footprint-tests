{
  "name": "shape-tree",
  "version": "0.0.0",
  "keywords": [
    "solid",
    "shape trees",
    "shex",
    "shapes"
  ],
  "author": "eric@w3.org",
  "repository": "git@github.com:janeirodigital/shapetrees",
  "license": "ISC",
  "scripts": {
    "coverage:report": "cat ./coverage/lcov.info | coveralls",
    "genkeys": "openssl req -outform PEM -keyform PEM -new -x509 -sha256 -newkey rsa:2048 -nodes -keyout ./server.key -days 365 -out ./server.cert",
    "prepare-tests": "mkdir -p www && ls -d www/* | grep -v README.md | xargs rm -rf",
    "serve:ldp": "PORT=12340 DEBUG=LDP*,send ./bin/runServer LDP",
    "serve:app": "PORT=12340 DEBUG=AppStore*,send ./bin/runServer AppStore",
    "serve": "echo 'goto https://localhost:12340/playground/index.html?manifest=../console/manifest.json' && PORT=12340 DEBUG=LDP*,AppStore* ./bin/runServer LDP AppStore console:console playground:playground",
    "test": "npm run prepare-tests && PORT=12345 nyc mocha --slow 200",
    "test:quiet": "npm run prepare-tests && PORT=12345 nyc mocha --slow 200 --reporter mocha-slow-options --reporter-options useReporter=spec,mediumPercent=100 --sort test/*.test.js",
    "update-www-repo": "PORT=12345 ./update-www-repo.sh",
    "start": "node ./bin/www",
    "webpack": "webpack"
  },
  "dependencies": {
    "@shexjs/cli": "^1.0.0-alpha.8",
    "@shexjs/core": "^1.0.0-alpha.8",
    "@shexjs/parser": "^1.0.0-alpha.8",
    "chai-as-promised": "^7.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "express": "~4.16.1",
    "fs-extra": "^8.1.0",
    "jsonld": "^1.8.1",
    "mocha": "^7.1.1",
    "mocha-slow-options": "0.0.2",
    "morgan": "~1.9.1",
    "n3": "^1.3.0",
    "node-fetch": "^2.6.0",
    "relateurl": "^0.2.7",
    "request-promise": "^4.2.5",
    "serve-favicon": "^2.5.0",
    "serve-index": "^1.9.1",
    "uri-template-lite": "^19.11.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.7",
    "debug": "^4.1.1",
    "jest-silent-reporter": "^0.1.2",
    "nyc": "^15.0.0",
    "superagent": "^5.1.0"
  },
  "nyc": {
    "exclude": [
      "util/rdf-serialization.js",
      "test/*"
    ]
  }
}
