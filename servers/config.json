{
  "AppStore": {
    "description": "app static content", "bin": "AppStore.js", "documentRoot": "solidApps/staticRoot",
    "protocol": "V  V - shapetree.js/storage/fetch-self-signed should allow the LDP server to talk to an HTTPS AppStore server but it doesn't, so use http here:",
    "protocol": "http",
    "keyFilePath": "server.key",
    "certFilePath": "server.cert"
  },
  "LDP": {
    "description": "toy LDP server",     "bin": "LDP.js",      "documentRoot": "www",
    "protocol": "https",
    "keyFilePath": "servers/ssl.key",
    "certFilePath": "servers/ssl.cert",
    "indexFile": "_self.ttl",
    "cache": "Cache",
    "shared": "Data",
    "apps": "Apps"
  }
}
